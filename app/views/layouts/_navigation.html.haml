#user_nav
  .container
    .logo.pull-left= link_to "Jupiter", root_url
    .pull-right
      - if current_user
        Signed in as
        = succeed "" do
          %strong{style: "margin-right: 20px;"}= current_user.name
        = link_to "Sign out", signout_path, id: "sign_out", class: "google_sign_in"
      - else
        = link_to "Sign in with Google", "/auth/google_oauth2", class: "google_sign_in"